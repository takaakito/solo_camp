
<h1>投稿してみよう！！！</h1>
  <%= form_with model: @camp do |f| %>
    <h4>画像</h4>
      <%= f.file_field :image, accept: "image/*" %>
    <h4>キャンプ場の名前</h4>
      <%= f.text_field :name %>
    <h4>エリア</h4>
     <%= f.select :camp_area, Camp.camp_areas.keys, {}, {class: 'hoge'}%>
    <h4>キャンプの種類</h4>
     <%= f.select :category, Camp.categories.keys, {}, {class: 'hoge'}%>
    <h4>説明</h4>
      <%= f.text_area :body %>
    <%= f.submit '投稿' %>
  <% end %>